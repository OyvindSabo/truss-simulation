{
  "name": "stylic",
  "version": "0.0.1",
  "author": "Erik Fox <erik@erikfox.co> (https://github.com/erikfox)",
  "description": "A template for created open source node packages.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/erikfox/stylic",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikfox/stylic.git"
  },
  "bugs": {
    "url": "https://github.com/erikfox/stylic/issues"
  },
  "keywords": [],
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm run build -- --watch & npm run bundle -- --watch & npm test -- --watchAll & wait",
    "build": "babel ./src/index.js --out-file ./lib/index.js",
    "bundle": "webpack --config webpack.config.js",
    "test": "jest --coverage",
    "lint": "eslint src",
    "format": "prettier --single-quote --trailing-comma=es5 --write src/**/*.js",
    "precommit": "npm run format && npm run lint && npm run test && npm run prepublish && git add src lib",
    "prepublish": "npm run build && npm run bundle"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "pascal-case": "^2.0.1",
    "prettier": "^1.5.3",
    "webpack": "^3.5.2"
  },
  "directories": {
    "lib": "lib"
  }
}
